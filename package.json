{
  "name": "Food_Pantry",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "sass": "sass --watch src/assets/scss:src/assets/css",
    "md-dist": "mkdir dist",
    "md-assets": "cd dist && mkdir assets",
    "md-css": "cd dist/assets && mkdir css",
    "md-img": "cd dist/assets && mkdir images",
    "cpy-html": "cp -r ./src/*.html ./dist",
    "cpy-css": "cp -r ./src/assets/css/*.* ./dist/assets/css",
    "cpy-img": "cp -r ./src/assets/images/*.* ./dist/assets/images",
    "new-dir": "npm run md-dist && npm run md-assets && npm run md-css && npm run md-img",
    "copy-files": "npm run cpy-html && npm run cpy-css && npm run cpy img"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "parcel-bundler": "^1.12.5",
    "sass": "^1.37.2"
  },
  "dependencies": {
    "autoprefixer": "^10.3.1",
    "bootstrap": "^4.6.0",
    "jquery": "^3.6.0",
    "postcss-cli": "^8.3.1"
  }
}
